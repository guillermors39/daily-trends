  "description": "API to get news feeds",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/src/main.js",
    "start:dev": "tsnd --respawn src/main.ts",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --coverage",
    "deps:check": "depcruise src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillermors39/daily-trends.git"
  },
  "keywords": [
    "daily",
    "news",
    "trends"
  ],
  "author": "Guillermo Rolon Soto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guillermors39/daily-trends/issues"
  },
  "homepage": "https://github.com/guillermors39/daily-trends#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dependency-cruiser": "^16.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-status": "^1.7.4",
    "joi": "^17.12.2",
    "mongoose": "^8.2.0",
    "puppeteer": "^22.3.0"
  },
  "lint-staged": {
    "*.(js|ts)": "lint:fix"
  }
